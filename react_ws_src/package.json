{
  "name": "XTTT-react-client",
  "description": "X Tic Tac Toe game demo front end built with React.js stack",
  "version": "0.0.1",
  "author": "Maxim Shklyar, kisla interactive, <maxim@kisla.com>",
  "private": true,
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/xims/X-TTT/react_ws_src"
  },
  "scripts": {
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "jest --coverage",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:webpack_dev": "cross-env NODE_ENV=development webpack --config webpack.config.prod.js",
    "bc": "npm run build && npm run build:copyall_win",
    "bu": "npm run build && npm run build:copyupd_win",
    "bv": "npm version patch",
    "build:copyall_win": "xcopy dist\\* ..\\WS\\public /E /Y /D && xcopy static\\* ..\\WS\\public /E /Y /D",
    "build:copyupd_win": "copy dist\\bundle.js ..\\WS\\public\\ && copy dist\\style.css ..\\WS\\public\\"
  },
  "dependencies": {
    "ampersand-app": "^2.0.0",
    "get-size": "^2.0.2",
    "gsap": "^1.18.5",
    "jquery": "^3.1.0",
    "object-assign": "^4.1.0",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-ga": "^2.1.1",
    "react-imageloader": "^2.1.0",
    "react-motion": "^0.4.4",
    "react-router": "^2.5.2",
    "socket.io": "^1.4.8",
    "superagent": "^2.0.0",
    "validator": "^5.4.0",
    "x2js": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^13.2.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "jest-cli": "^13.2.3",
    "node-sass": "^7.0.1",
    "proxy-middleware": "^0.15.0",
    "react-addons-test-utils": "^15.2.0",
    "rimraf": "^2.5.3",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  }
}
